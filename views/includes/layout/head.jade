- var minifier = '';
if ui.minifier
  - minifier = '.min'
head
  meta(charset='utf8')
  meta(http-equiv='X-UA-Compatible' content='IE=edge')
  meta(name='viewport' content='width=device-width user-scalable=no')
  base(href='/')
  title= __('AppName')
  link(rel='stylesheet' href='/stylesheets/bootstrap.min.css')
  if ui.datetimePicker
    link(rel='stylesheet' href='/stylesheets/bootstrap-datetimepicker.min.css')
  link(rel='stylesheet' href='/stylesheets/font-awesome.min.css')
  link(rel='stylesheet' href='/stylesheets/flag-icon.min.css')
  if ui.minicolors
    link(rel='stylesheet' href='/stylesheets/jquery.minicolors.min.css')
  if ui.editor
    link(rel='stylesheet' href='/stylesheets/textAngular.min.css')
  if ui.switch
    link(rel='stylesheet' href='/stylesheets/angular-toggle-switch.min.css')
  if ui.uploader
    link(rel='stylesheet' href='/stylesheets/blueimp-gallery.min.css')
    link(rel='stylesheet' href='/stylesheets/jquery.fileupload.min.css')
    link(rel='stylesheet' href='/stylesheets/jquery.fileupload-ui.min.css')
  if ui.tree
    link(rel='stylesheet' href='/stylesheets/angular-ui-tree.min.css')
  if ui.multiSelect
    link(rel='stylesheet' href='/stylesheets/angular-isteven-multi-select.min.css')
  if ui.rangeSlider
    link(rel='stylesheet' href='/stylesheets/angular-range-slider.min.css')
  link(rel='stylesheet' href='/stylesheets/dialogs.min.css')
  link(rel='stylesheet' href='/stylesheets/app-animation.min.css')
  link(rel='stylesheet' href='/stylesheets/vulpe.min.css')
  link(rel='stylesheet' href='/stylesheets/app.min.css')
  script(src='/javascripts/moment.min.js' type='text/javascript')
  script(src='/javascripts/humanize-duration.min.js' type='text/javascript')
  script(src='/javascripts/jquery/jquery.min.js' type='text/javascript')
  script(src='/javascripts/jquery/jquery.cookie.min.js' type='text/javascript')
  script(src='/javascripts/jquery/jquery.hotkeys.min.js' type='text/javascript')
  if ui.minicolors
    script(src='/javascripts/jquery/jquery.minicolors.min.js' type='text/javascript')
  if ui.uploader
    script(src='/javascripts/jquery/uploader/jquery.ui.widget.js' type='text/javascript')
    script(src='/javascripts/jquery/uploader/load-image.all.min.js' type='text/javascript')
    script(src='/javascripts/jquery/uploader/canvas-to-blob.min.js' type='text/javascript')
    script(src='/javascripts/jquery/uploader/jquery.blueimp-gallery.min.js' type='text/javascript')
    script(src='/javascripts/jquery/uploader/jquery.iframe-transport.min.js' type='text/javascript')
    script(src='/javascripts/jquery/uploader/jquery.fileupload.min.js' type='text/javascript')
    script(src='/javascripts/jquery/uploader/jquery.fileupload-process.min.js' type='text/javascript')
    script(src='/javascripts/jquery/uploader/jquery.fileupload-image.min.js' type='text/javascript')
    script(src='/javascripts/jquery/uploader/jquery.fileupload-audio.min.js' type='text/javascript')
    script(src='/javascripts/jquery/uploader/jquery.fileupload-video.min.js' type='text/javascript')
    script(src='/javascripts/jquery/uploader/jquery.fileupload-validate.min.js' type='text/javascript')
  script(src='/javascripts/bootstrap/bootstrap.min.js' type='text/javascript')
  if ui.datetimePicker
    script(src='/javascripts/bootstrap/bootstrap-datetimepicker.min.js' type='text/javascript')
  script(src='/javascripts/bootstrap/bootstrap-hover-dropdown.min.js' type='text/javascript')
  script(src='/javascripts/angular/angular.min.js' type='text/javascript')
  script(src='/javascripts/angular/angular-animate.min.js' type='text/javascript')
  script(src='/javascripts/angular/angular-cookies.min.js' type='text/javascript')
  script(src='/javascripts/angular/angular-sanitize.min.js' type='text/javascript')
  script(src='/javascripts/angular/angular-timer.min.js' type='text/javascript')
  script(src='/javascripts/angular/angular-i18n-node.min.js' type='text/javascript')
  script(src='/javascripts/angular/angular-translate.min.js' type='text/javascript')
  if ui.flow
    script(src='/javascripts/angular/angular-flow-uploader.min.js' type='text/javascript')
  if ui.minicolors
    script(src='/javascripts/angular/angular-minicolors.min.js' type='text/javascript')
  script(src='/javascripts/angular/angular-masks.min.js' type='text/javascript')
  if ui.switch
    script(src='/javascripts/angular/angular-toggle-switch.min.js' type='text/javascript')
  script(src='/javascripts/angular/loading-bar.min.js' type='text/javascript')
  script(src='/javascripts/angular/dirPagination.min.js' type='text/javascript')
  if ui.editor
    script(src='/javascripts/angular/textAngular.min.js' type='text/javascript')
    script(src='/javascripts/angular/textAngular-sanitize.min.js' type='text/javascript')
  if ui.tree
    script(src='/javascripts/angular/angular-ui-tree.min.js' type='text/javascript')
  if ui.multiSelect
    script(src='/javascripts/angular/angular-isteven-multi-select.min.js' type='text/javascript')
    script(src='/javascripts/angular/angular-dropdown-multiselect.min.js' type='text/javascript')
  if ui.rangeSlider
    script(src='/javascripts/angular/angular-range-slider.min.js' type='text/javascript')
  if ui.dragDrop
    script(src='/javascripts/angular/angular-drag-and-drop-lists.min.js' type='text/javascript')
  if ui.recaptcha
    script(src='/javascripts/angular/angular-recaptcha.min.js' type='text/javascript')
    script(src='http://www.google.com/recaptcha/api.js?render=explicit&onload=vcRecaptchaApiLoaded' async defer)
  script(src='/javascripts/angular/ui-bootstrap-tpls.min.js' type='text/javascript')
  script(src='/javascripts/angular/ui-utils.min.js' type='text/javascript')
  script(src='/javascripts/angular/dialogs.min.js' type='text/javascript')
  if ui.uploader
    script(src='/javascripts/jquery/uploader/jquery.fileupload-angular.min.js' type='text/javascript')
  script(src='/javascripts/app/directives#{minifier}.js' type='text/javascript')
  script(src='/javascripts/app/filters#{minifier}.js' type='text/javascript')
  script(src='/javascripts/app/services#{minifier}.js' type='text/javascript')
  script(src='/javascripts/app/controllers#{minifier}.js' type='text/javascript')
  script(src='/javascripts/vulpe#{minifier}.js' type='text/javascript')
  script(src='/javascripts/app#{minifier}.js' type='text/javascript')
  if ui.flow
    script(src='/javascripts/flow.min.js' type='text/javascript')
    script(src='/javascripts/fusty-flow.min.js' type='text/javascript')
  if ui.charts
    script(src="/javascripts/highcharts/highcharts.js")
    script(src="/javascripts/highcharts/modules/exporting.js")
  if ui.uploader
    script(src="/javascripts/jquery/cors/jquery.postmessage-transport.js")
    <!--[if (gte IE 8)&(lt IE 10)]>
    <script src="/javascripts/jquery/cors/jquery.xdr-transport.min.js"></script>
    <![endif]-->
  include ../app
